  trakkGetClientIDfromURL().returnClientID();

  //get customized client id
  function trakkGetClientIDfromURL() {
    var gaClientidParameterKey = "gaclientid";
    var clickTimestampParameterKey = "gaclientidtime";
    var dataLayerEventName = "gaClientIDreceived";
    return {
      returnClientID: returnClientID
    };

    function returnClientID() {
      var clientID = getClientIdFromURL();
      var clickTimestamp = getClientIdTimeStamp() || 0;
      var currentTimeStamp = Date.now() / 1000;

      if (currentTimeStamp - clickTimestamp > 60) {
        clientID = undefined;
      }
      pushClientIDtoDataLayer(clientID);
      return clientID;
    }

    function pushClientIDtoDataLayer(clientID) {
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push({
        event: dataLayerEventName,
        clientID: clientID
      });
    }

    function getClientIdFromURL() {
      var urlParam = window.location.search.substring(1);
      var paramSplit = urlParam.split("&");

      for (var i = 0; i < paramSplit.length; i++) {
        if (paramSplit[i].indexOf("gaclientid") > -1) {
          var clientId = paramSplit[i].split("=")[1];
          return clientId;
        }
      }
    }

    function getClientIdTimeStamp() {
      var urlParam = document.location.search;
      var paramSplit = urlParam.split("&");
      for (var i = 0; i < paramSplit.length; i++) {
        if (paramSplit[i].indexOf("gaclientidtime") > -1) {
          var clientIdTimestamp = paramSplit[i].split("=")[1];
          return clientIdTimestamp;
        }
      }
    }
  }

  (function(w, d, s, l, i) {
    w[l] = w[l] || [];
    w[l].push({ "gtm.start": new Date().getTime(), event: "gtm.js" });
