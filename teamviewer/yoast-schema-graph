jQuery("#menu-item-183231 a, #menu-item-183232 a, .zz_button, .zz_hero__alt a, .zz_card-list__card").click(function (e) {        
    e.preventDefault(); //cancel the link click
    var url = jQuery(this).attr('href');//get the destination url of the link
    setTimeout(function () { window.location.href = url; }, 250); 
});

