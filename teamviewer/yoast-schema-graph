  const dataLayerEvent = "^OptanonLoaded$";
  const dataLayerEventRegexp = new RegExp(dataLayerEvent);
  const onetrustActiveGroups = "^,1,2,";
  const onetrustActiveGroupsRegexp = new RegExp(onetrustActiveGroups);

  function writeExpCookie() {
    var urlParam = document.location.search;
    var paramSplit = urlParam.split("&");
    var _gaexp = getParameter("_gaexpadserver", window.location.href);
    for (var i = 0; i < paramSplit.length; i++) {
      if (paramSplit[i].indexOf("_gaexpadserver") > -1 && _gaexp !== "noexp") {
        createCookie("_gaexp", _gaexp, 90);
      }
    }
  }

  function getParameter(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
      results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return "";
    return decodeURIComponent(results[2].replace(/\+/g, " "));
  }

  function createCookie(name, value, days) {
    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
      var expires = "; expires=" + date.toGMTString();
    } else var expires = "";
    document.cookie = name + "=" + value + expires + "; path=/";
  }

  if (checkConsent() === true) {
    injectOptimiseSnippet();
  } else {
    window.dataLayer = window.dataLayer || [];
    const originPush = window.dataLayer.push
    dataLayer.push = function(item) {
        originPush.apply(window.dataLayer, arguments);
        const pushEvent = new CustomEvent("dataLayerPush", {
            detail: item
        });
        window.dispatchEvent(pushEvent);
    }
    window.addEventListener("dataLayerPush", function (dataLayer) {
      if (dataLayerEventRegexp.test(dataLayer.detail.event) && onetrustActiveGroupsRegexp.test(dataLayer.detail.OptanonActiveGroups)) {
          injectOptimiseSnippet();
      }
  });
  }

  function injectOptimiseSnippet() {
    (function(a, s, y, n, c, h, i, d, e) {
      s.className += " " + y;
      h.start = 1 * new Date();
      h.end = i = function() {
        s.className = s.className.replace(RegExp(" ?" + y), "");
      };
      (a[n] = a[n] || []).hide = h;
      setTimeout(function() {
        i();
        h.end = null;
      }, c);
      h.timeout = c;
    })(window, document.documentElement, "async-hide", "dataLayer", 4000, {
      "OPT-KJWK84R": true
    });

    (function(i, s, o, g, r, a, m) {
      i["GoogleAnalyticsObject"] = r;
      (i[r] =
        i[r] ||
        function() {
